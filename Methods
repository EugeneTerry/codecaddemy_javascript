// Use .toUpperCase() to log 'Codecademy' in all uppercase letters
console.log('Codecademy'.toUpperCase()); //the ".toUpperCase()" made it display 'CODECADEMY'

// Use a string method to log the following string without whitespace at the beginning and end of it.
console.log('    Remove whitespace   '.trim()); //the ".trim()" made it display it without the spaces infront of behind 
